<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
	  <th>Options</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>Name: <%= user.name %><br />Email:<%= user.email %></td>
        <td><%= link_to 'Show', user %><br/>
        <%= link_to 'Edit', edit_user_registration_path(user) %><br/>
        <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
